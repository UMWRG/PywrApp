<?xml version="1.0"?>
<template_definition>
<template_name>PywrNetwork</template_name>
<layout>	    
    <item>
		<name>grouping</name>
            <value>			    
			    <name>PywrNetwork</name>
				<description>Generic Pywr network</description>
				<categories>
				    <category>
						<name>Nodes</name>
						<description>Nodes</description>
						<displayname>Nodes</displayname>
						<groups>
							<group>
								<name>output</name>
								<description>output node</description>
								<displayname>output node</displayname>
								<image>images\output.png</image>
							</group>
							<group>
								<name>input</name>
								<description>input node</description>
								<displayname>input node</displayname>
								<image>images\input.png</image>
							</group>
							<group>
								<name>aggregatednode</name>
								<description>aggregatednode node</description>
								<displayname>aggregatednode node</displayname>
								<image>images\aggregatednode.png</image>
							</group>
							<group>
								<name>catchment</name>
								<description>catchment node</description>
								<displayname>catchment node</displayname>
								<image>images\catchment.png</image>
							</group>
                            <group>
								<name>storage</name>
								<description>storage node</description>
								<displayname>storage node</displayname>
								<image>images\storage.png</image>
							</group>
                            <group>
								<name>reservoir</name>
								<description>reservoir node</description>
								<displayname>reservoir node</displayname>
								<image>images\reservoir.png</image>
							</group>                                                        							
                        </groups>
                    </category>
                    <category>
                            <name>links</name>
                            <description>links</description>
                            <displayname>links</displayname>
                        <groups>
                            <group>
								<name>edges</name>
								<description>edge</description>
								<displayname>edge</displayname>		
								<image>images\edges.png</image>
                            </group>							
							<group>
								<name>river</name>
								<description>river node</description>
								<displayname>river node</displayname>
								<image>images\river.png</image>
							</group>
							<group>
								<name>link</name>
								<description>link node</description>
								<displayname>link node</displayname>
								<image>images\link.png</image>
							</group>
						</groups>
				    </category>	
				</categories>
            </value>
        </item>
    </layout>
	
<resources>
    <resource>
        <type>NETWORK</type>
        <name>PywrNetwork</name>        
    </resource>  	
	<resource>
        <type>NODE</type>
        <name>aggregatednode</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\aggregatednode.png</value>
            </item>
            <item>
                <name>group</name>
                <value>aggregatednode</value>
            </item>
        </layout>
        <attribute>
            <name>factors</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>array</data_type>
        </attribute>
        <attribute>
            <name>nodes</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>array</data_type>
        </attribute>   
		<attribute>
            <name>min_flow</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute> 		
    </resource>
    <resource>
        <type>NODE</type>
        <name>output</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\output.png</value>
            </item>
            <item>
                <name>group</name>
                <value>output</value>
            </item>
        </layout>
        <attribute>
            <name>received_water</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>
		<attribute>
            <name>totaldeficitnode</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>array</data_type>
        </attribute>
		<attribute>
            <name>totalflownode</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>array</data_type>
        </attribute>	
        <attribute>
            <name>max_flow</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute>   
		<attribute>
            <name>min_flow</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute> 		
    </resource>
	 <resource>
        <type>NODE</type>
        <name>input</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\input.png</value>
            </item>
            <item>
                <name>group</name>
                <value>input</value>
            </item>
        </layout>
        <attribute>
            <name>max_flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute>    
		<attribute>
            <name>min_flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute> 
		<attribute>
            <name>mean_flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>    		
    </resource>
	
    <resource>
        <type>NODE</type>
        <name>catchment</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\catchment.png</value>
            </item>
            <item>
                <name>group</name>
                <value>catchment</value>
            </item>
        </layout>
        <attribute>
            <name>flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute> 
		<attribute>
            <name>seasonal_fdc</name>
			<dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute> 		
    </resource>
	    <resource>
        <type>NODE</type>
        <name>storage</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\storage.png</value>
            </item>
            <item>
                <name>group</name>
                <value>storage</value>
            </item>
        </layout>		
		<attribute>
            <name>polynomial1dcost</name>
            <dimension>Dimensionless</dimension>			
            <is_var>Y</is_var>
            <data_type>array</data_type>
        </attribute>
       <attribute>
            <name>storage</name>
            <dimension>Dimensionless</dimension>			
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>
        <attribute>
            <name>max_storage</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>scalar</data_type>
        </attribute>
        <attribute>
            <name>min_storage</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>		
        </attribute>
		 <attribute>
            <name>initial_volume</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>		
        </attribute>
    </resource>
    <resource>
        <type>NODE</type>
        <name>reservoir</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\reservoir.png</value>
            </item>
            <item>
                <name>group</name>
                <value>reservoir</value>
            </item>
        </layout>

        <attribute>
            <name>storage</name>     
            <dimension>Dimensionless</dimension>   			
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>
        <attribute>
            <name>max_volume</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>scalar</data_type>
        </attribute>
        <attribute>
            <name>min_volume</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>		
        </attribute>
		<attribute>
            <name>initial_volume</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>		
        </attribute>
		 <attribute>
            <name>reservoir_area</name>     
            <dimension>Dimensionless</dimension>   			
            <is_var>N</is_var>
            <data_type>array</data_type>
        </attribute>
    </resource>
	<resource>
        <type>LINK</type>
        <name>link</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\link.png</value>
            </item>
            <item>
                <name>group</name>
                <value>link</value>
            </item>
        </layout>		
		<attribute>
            <name>max_flow</name>
			<dimension>Dimensionless</dimension>
             <is_var>N</is_var>
            <data_type>timeseries</data_type>
       </attribute>
        <attribute>
            <name>min_flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>			
        </attribute>
        <attribute>
            <name>flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>      
    </resource> 
     <resource>
        <type>LINK</type>
        <name>river</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\river.png</value>
            </item>
            <item>
                <name>group</name>
                <value>river</value>
            </item>
        </layout>		
		<attribute>
            <name>max_flow</name>
			<dimension>Dimensionless</dimension>
             <is_var>N</is_var>
            <data_type>timeseries</data_type>
       </attribute>
        <attribute>
            <name>min_flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>			
        </attribute>
        <attribute>
            <name>flow</name>
			<dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
            <data_type>timeseries</data_type>
        </attribute>      
    </resource>  	
    <resource>
        <type>LINK</type>
        <name>edge</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\RedSolidLine16.png</value>
            </item>
			<item>
					<name>symbol</name>
					<value>solid</value>
				</item>
				<item>
					<name>colour</name>
					<value>red</value>
				</item>
				<item>
					<name>line_weight</name>
					<value>1</value>
				</item>
            <item>
                <name>group</name>
                <value>pipe</value>
            </item>
        </layout>        
    </resource>    
</resources>
</template_definition>

